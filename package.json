{
  "name": "schema-dts-gen",
  "version": "0.5.0",
  "displayName": "schema-dts Generator",
  "description": "Generate TypeScript Definitions for Schema.org Schema",
  "authors": [
    "Eyas Sharaiha <eyas@google.com> (https://eyas.sh/)"
  ],
  "maintainers": [
    "Eyas Sharaiha <npm@eyas.sh> (https://eyas.sh/)"
  ],
  "files": [
    "dist/gen/**/*"
  ],
  "types": "./dist/gen/index.d.ts",
  "main": "./dist/gen/index.js",
  "bin": "./dist/gen/cli/cli.js",
  "devDependencies": {
    "@types/argparse": "^1.0.36",
    "@types/chai": "^4.2.7",
    "@types/diff": "^3.5.2",
    "@types/htmlparser2": "^3.10.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.0",
    "chai": "^4.2.0",
    "child_process": "^1.0.2",
    "clang-format": "^1.2.4",
    "coveralls": "^3.0.9",
    "del": "^3.0.0",
    "diff": "^3.5.0",
    "gulp": "^4.0.2",
    "gulp-file": "^0.4.0",
    "gulp-typescript": "^5.0.1",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "sinon": "^8.0.4",
    "source-map-support": "^0.5.16",
    "through2": "^3.0.1",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "argparse": "^1.0.10",
    "htmlparser2": "^3.10.1",
    "rxjs": "^6.5.3"
  },
  "peerDependencies": {
    "typescript": "^3.4.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist/**/*",
      "gulpfile.js",
      "coverage/**/*",
      "test/**/*"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "keywords": [
    "typescript",
    "tsd",
    "dts",
    "schema.org",
    "Semantic Web",
    "semantic-web",
    "Linked Data",
    "linked-data",
    "jsonld",
    "JSON-LD",
    "structured data",
    "structured-data"
  ],
  "homepage": "https://opensource.google/projects/schema-dts",
  "bugs": "https://github.com/google/schema-dts/issues",
  "repository": "github:google/schema-dts",
  "license": "Apache-2.0",
  "scripts": {
    "build-gen": "tsc -p src",
    "test": "nyc mocha",
    "coverage_on_travis": "nyc report --reporter=text-lcov | coveralls"
  }
}
